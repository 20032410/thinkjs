{
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "rules": {
    "strict": [0],
    "eqeqeq": 2,
    "quotes": [2, "single"],
    "no-underscore-dangle": 0,
    "eol-last": 0,
    "camelcase": 0,
    "no-loop-func": 0,
    "no-trailing-spaces": 0,
    "consistent-return": 0,
    "new-cap": 0,
    "no-shadow": 0,
    //"semi": 0,
    "no-process-exit": 0
  },
  "globals": {
    "think": true,
    "thinkCache": true
  }
}